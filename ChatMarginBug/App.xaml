<Application
    x:Class="ChatMarginBug.App"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:converters="using:ChatMarginBug.Converters"
    xmlns:local="using:ChatMarginBug">
    <Application.Resources>
        <x:Double x:Key="TinyFontSize">12</x:Double>
        <x:Double x:Key="SmallFontSize">16</x:Double>
        <x:Double x:Key="MediumFontSize">20</x:Double>
        <converters:TweetTypeToColorConverter x:Key="TweetTypeToColorConverter1" />
        <converters:TweetTypeToHAConverter x:Key="TweetTypeToHAConverter1" />
        <converters:TweetTypeToC1DefConverter x:Key="TweetTypeToC1DefConverter1" />
        <converters:TweetTypeToC2DefConverter x:Key="TweetTypeToC2DefConverter1" />
        <converters:TweetTypeToMetaDataColumnConverter x:Key="TweetTypeToMetaDataColumnConverter1" />
        <converters:TweetTypeToMsgColumnConverter x:Key="TweetTypeToMsgColumnConverter1" />
    </Application.Resources>
</Application>
